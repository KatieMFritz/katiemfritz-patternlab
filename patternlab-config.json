{
  "paths" : {
    "source" : {
      "root": "./source/",
      "patterns" : "./source/_patterns/",
      "data" : "./source/_data/",
      "meta": "./source/_meta/",
      "annotations" : "./source/_annotations/",
      "styleguide" : "./node_modules/styleguidekit-assets-default/dist/",
      "patternlabFiles" : "./node_modules/styleguidekit-mustache-default/views/",
      "js" : "./source/js",
      "images" : "./source/images",
      "fonts" : "./source/css/fonts/",
      "css" : "./source/css/",
      "scss" : "./source/css/scss/"
    },
    "public" : {
      "root" : "./public/",
      "patterns" : "./public/patterns/",
      "data" : "./public/styleguide/data/",
      "annotations" : "./public/annotations/",
      "styleguide" : "./public/styleguide/",
      "js" : "./public/js",
      "images" : "./public/images",
      "fonts" : "./public/css/fonts",
      "css" : "./public/css"
    },
    "prod" : {
      "root" : "../katiemfritz-nuxt/",
      "css" : "../katiemfritz-nuxt/assets/css/",
      "js" : "../katiemfritz-nuxt/assets/js/",
      "images" : "../katiemfritz-nuxt/assets/images/",
      "fonts" : "../katiemfritz-nuxt/assets/css/fonts/"
    }
  },
  "colors" : {
    "paths" : {
      "scss" : "./source/css/scss/00_base/",
      "json" : "./source/_patterns/00-global/00-colors/"
    },
    "filenames" : {
      "scss" : "_colors.scss",
      "json" : "00-colors.json"
    }
  },
  "styleGuideExcludes": [
  ],
  "defaultPattern": "all",
  "defaultShowPatternInfo": false,
  "cleanPublic" : true,
  "patternExtension": "mustache",
  "ignored-extensions" : ["scss", "DS_Store", "less"],
  "ignored-directories" : ["scss"],
  "debug": false,
  "ishControlsHide": {
    "s": false,
    "m": false,
    "l": false,
    "full": false,
    "random": false,
    "disco": false,
    "hay": true,
    "mqs": false,
    "find": false,
    "views-all": false,
    "views-annotations": false,
    "views-code": false,
    "views-new": false,
    "tools-all": false,
    "tools-docs": false
  },
  "ishMinimum": "240",
  "ishMaximum": "2600",
  "patternStateCascade": ["inprogress", "inreview", "complete"],
  "patternStates": {
  },
  "patternExportPatternPartials": ["atoms-arrow", "atoms-bubble", "molecules-banner", "molecules-cta", "molecules-intro", "organisms-header", "organisms-footer"],
  "patternExportDirectory": "./pattern_exports/",
  "cacheBust": true,
  "starterkitSubDir": "dist",
  "outputFileSuffixes": {
    "rendered": ".rendered",
    "rawTemplate": "",
    "markupOnly": ".markup-only"
  },
  "cleanOutputHtml": true
}
